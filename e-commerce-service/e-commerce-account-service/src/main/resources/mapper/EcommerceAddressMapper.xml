<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.ecommerce.mapper.EcommerceAddressMapper">

    <select id="findAllByUserId" resultType="com.imooc.ecommerce.entity.EcommerceAddress">
        select t.* from t_ecommerce_address t where t.user_id = #{userId}
    </select>
</mapper>
