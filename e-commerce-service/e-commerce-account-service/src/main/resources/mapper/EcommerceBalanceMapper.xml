<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.ecommerce.mapper.EcommerceBalanceMapper">

    <select id="findBalanceByUserId" resultType="com.imooc.ecommerce.entity.EcommerceBalance">
        select t.* from t_ecommerce_balance t where t.user_id = #{userId}
    </select>
</mapper>
